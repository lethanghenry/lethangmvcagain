

@model Basic_level2_agan.Models.Blog

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <title></title>
    <style>
        body {
            padding: 0px;
            margin: 0px;
        }
    </style>
</head>
<body>
    <div>
        <div class="header row col-md-12" style="padding: 0px; margin: 0px; width: 100%;background:black;height:50px;">
            @Html.Partial("Header")
        </div>
        <div class="main row col-md-12" style="padding:0px;margin-right:10px;">
            <div class="menu col-md-2">
                @Html.Partial("Menu")
            </div>
            <div class="content col-md-10" style="padding:10px;margin:0px;background:#696969;">
                @Html.Partial("_Edit")
            </div>

        </div>
        <div class="col-md-12" style="padding:0px;">
            @Html.Partial("Footer")
        </div>
    </div>

</body>
</html>
