@model Basic_level2_agan.Models.Blog

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!DOCTYPE html>
<html>
<head>
    <title></title>
    <style>
        body {
            padding: 0px;
            margin: 0px;
        }
        .header {
            padding: 0px;
            margin: 0px;
            width: 100%;
            background: black;
            height: 50px;
        }
        .main {
            padding: 0px;
            margin-right: 10px;
        }
        .content {
            padding: 0px;
            margin: 0px;
        }
        .footer{
            padding:0px;
        }
    </style>
</head>
<body>
    <div>
        <div class="header row col-md-12">
            @Html.Partial("Header")
        </div>
        <div class="main row col-md-12" >
            <div class="menu col-md-2">
                @Html.Partial("Menu")
            </div>
            <div class="content col-md-10" >
                @Html.Partial("_Create")
            </div>
        </div>
        <div class="footer col-md-12" >
            @Html.Partial("Footer")
        </div>
    </div>

</body>
</html>

